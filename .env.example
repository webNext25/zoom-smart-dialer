# Environment Variables Template
# Copy this to .env.local and fill in your actual values

# Database
DATABASE_URL="postgresql://user:password@localhost:5432/zoom_dialer"

# Authentication
NEXTAUTH_SECRET="generate-this-with: openssl rand -base64 32"
NEXTAUTH_URL="http://localhost:3000"

# Settings Encryption (REQUIRED for admin settings system)
SETTINGS_ENCRYPTION_KEY="generate-this-with: openssl rand -base64 32"

# Vapi
NEXT_PUBLIC_VAPI_KEY="your-vapi-public-key-here"
VAPI_PRIVATE_KEY="your-vapi-private-key-here"

# ElevenLabs
NEXT_PUBLIC_ELEVENLABS_VOICE_ID="bIHbv24MWmeRgasZH58o"
ELEVENLABS_API_KEY="your-elevenlabs-api-key"

# Zoom
ZOOM_CLIENT_ID="your-zoom-client-id"
ZOOM_CLIENT_SECRET="your-zoom-client-secret"
ZOOM_SDK_KEY="your-zoom-sdk-key"
ZOOM_SDK_SECRET="your-zoom-sdk-secret"

# OpenAI
OPENAI_API_KEY="your-openai-api-key"

# File Storage (UploadThing)
UPLOADTHING_SECRET="your-uploadthing-secret"
UPLOADTHING_APP_ID="your-uploadthing-app-id"
